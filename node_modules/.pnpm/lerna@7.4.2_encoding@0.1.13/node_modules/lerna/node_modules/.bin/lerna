#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/admin/Downloads/code/low-code/node_modules/.pnpm/lerna@7.4.2_encoding@0.1.13/node_modules/lerna/dist/node_modules:/Users/admin/Downloads/code/low-code/node_modules/.pnpm/lerna@7.4.2_encoding@0.1.13/node_modules/lerna/node_modules:/Users/admin/Downloads/code/low-code/node_modules/.pnpm/lerna@7.4.2_encoding@0.1.13/node_modules:/Users/admin/Downloads/code/low-code/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/admin/Downloads/code/low-code/node_modules/.pnpm/lerna@7.4.2_encoding@0.1.13/node_modules/lerna/dist/node_modules:/Users/admin/Downloads/code/low-code/node_modules/.pnpm/lerna@7.4.2_encoding@0.1.13/node_modules/lerna/node_modules:/Users/admin/Downloads/code/low-code/node_modules/.pnpm/lerna@7.4.2_encoding@0.1.13/node_modules:/Users/admin/Downloads/code/low-code/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cli.js" "$@"
else
  exec node  "$basedir/../../dist/cli.js" "$@"
fi
