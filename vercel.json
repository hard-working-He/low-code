{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "http://81.68.224.194:8080/api/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=63072000; includeSubDomains; preload"
        }
      ]
    }
  ]
}


